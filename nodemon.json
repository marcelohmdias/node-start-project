{
  "delay": "500",
  "env": {
    "NODE_ENV": "development"
  },
  "execMap": {
    "ts": "node --inspect -r module-alias/register -r sucrase/register src/index.ts"
  },
  "ignore": ["./node_modules/*", "./tests/*"],
  "options": {
    "extensions": ["js", "json", "ts"]
  },
  "restartable": "rs",
  "verbose": false,
  "watch": ["./"]
}
